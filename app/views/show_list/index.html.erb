<h1>景品として欲しいものリスト</h1>
<a class="btn btn-primary" href="#" role="button">登録</a>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>名前</th>
      <th>欲しいもの</th>
      <th>URL</th>
      <th>ひとこと</th>
    </tr>
  </thead>
  <tbody class="table-info">
  <% @wantlists.each_with_index do |wantlist, ix| %>
    <tr>
      <th scope="row"><%= ix + 1 %></th>
      <td><%= wantlist.name %></td>
      <td><%= wantlist.product %></td>
      <td><%= wantlist.url %></td>
      <td><%= wantlist.comment %></td>
    </tr>
  <% end %>
  </tbody>
</table>
